@article{wang2023,
  title = {{{PLSDA-batch}}: A Multivariate Framework to Correct for Batch Effects in Microbiome Data},
  shorttitle = {{{PLSDA-batch}}},
  author = {Wang, Yiwen and Lê Cao, Kim-Anh},
  date = {2023-03-01},
  journaltitle = {Briefings in Bioinformatics},
