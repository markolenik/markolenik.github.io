@article{wang2014,
  title = {Endogenous Release of 5-{{HT}} Modulates the Plateau Phase of {{NMDA-Induced}} Membrane Potential Oscillations in Lamprey Spinal Neurons},
  author = {Wang, Di and Grillner, Sten and Wallén, Peter},
  date = {2014-07},
