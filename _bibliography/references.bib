  shorttitle = {{{PyNN}}},
  author = {Davison, Andrew P. and Brüderle, Daniel and Eppler, Jochen and Kremkow, Jens and Muller, Eilif and Pecevski, Dejan and Perrinet, Laurent and Yger, Pierre},
  date = {2009-01},
