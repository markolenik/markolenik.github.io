@article{desposito2007,
  title = {From Cognitive to Neural Models of Working Memory},
  author = {D'Esposito, Mark},
  date = {2007-05},
  journaltitle = {Philosophical Transactions of the Royal Society B: Biological Sciences},
  volume = {362},
