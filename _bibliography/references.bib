@article{ritchie2015,
  title = {Coupled {{Changes}} in {{Brain White Matter Microstructure}} and {{Fluid Intelligence}} in {{Later Life}}},
  author = {Ritchie, Stuart J. and Bastin, Mark E. and Tucker-Drob, Elliot M. and Maniega, Susana Muñoz and Engelhardt, Laura E. and Cox, Simon R. and Royle, Natalie A. and Gow, Alan J. and Corley, Janie and Pattie, Alison and Taylor, Adele M. and Hernández, Maria del C. Valdés and Starr, John M. and Wardlaw, Joanna M. and Deary, Ian J.},
