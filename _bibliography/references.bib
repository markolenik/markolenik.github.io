@article{lynch2011,
  title = {The {{Likelihood}} of {{Cognitive Enhancement}}},
  author = {Lynch, Gary and Palmer, Linda C. and Gall, Christine M.},
  date = {2011-08},
  journaltitle = {Pharmacology, biochemistry, and behavior},
  volume = {99},
  number = {2},
  eprint = {21215768},
