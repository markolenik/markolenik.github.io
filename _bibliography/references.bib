@article{guertin1998,
  title = {Chemical and Electrical Stimulation Induce Rhythmic Motor Activity in an in Vitro Preparation of the Spinal Cord from Adult Turtles},
  author = {Guertin, Pierre A and Hounsgaard, Jorn},
  date = {1998-03},
  journaltitle = {Neuroscience Letters},
  volume = {245},
  number = {1},
  pages = {5--8},
  issn = {0304-3940},
  doi = {10.1016/S0304-3940(98)00164-5},
  urldate = {2016-07-12},
  abstract = {Bath application of N-Methyl-d-aspartate (NMDA) and serotonin induced rhythmic activity in hindlimb motoneurons in an isolated spinal cord preparation from adult turtles. Subthreshold concentrations of NMDA and serotonin accompanied by stimulation of the dorsal funiculus also induced rhythmic activity. The induced activity in motoneurons and ventral roots displayed characteristics which resemble that of locomotion. This activity could be evoked in as few as three segments (D8–D10) of the lumbar enlargement isolated from the rest of the spinal cord. The existence of intrinsic oscillations in single neurons was revealed in the presence of tetrodotoxin. Our findings introduce a new in vitro preparation in which electrophysiological and pharmacological tools can be used with ease to study mechanisms of central pattern generation in a mature spinal motor network.},
  keywords = {central pattern generator,locomotion,N-methyl-d-aspartate,oscillations,Serotonin},
  timestamp = {2016-07-12T08:48:58Z},
  file = {/Users/molenik/Zotero/storage/7SYXJFMM/guertin1998.pdf}
}

