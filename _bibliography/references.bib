@article{bucci2012,
  title = {Social {{Interaction}}, {{Noise}} and {{Antibiotic-Mediated Switches}} in the {{Intestinal Microbiota}}},
  author = {Bucci, Vanni and Bradde, Serena and Biroli, Giulio and Xavier, Joao B.},
  editor = {De Boer, Rob J.},
