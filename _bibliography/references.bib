@article{rybak2006,
  title = {Modelling Spinal Circuitry Involved in Locomotor Pattern Generation: Insights from Deletions during Fictive Locomotion},
  shorttitle = {Modelling Spinal Circuitry Involved in Locomotor Pattern Generation},
  author = {Rybak, Ilya A. and Shevtsova, Natalia A. and Lafreniere-Roula, Myriam and McCrea, David A.},
  date = {2006-12},
