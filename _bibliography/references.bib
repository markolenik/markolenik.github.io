@article{manning2010,
  title = {Coaction of Intercellular Adhesion and Cortical Tension Specifies Tissue Surface Tension},
  author = {Manning, M. Lisa and Foty, Ramsey A. and Steinberg, Malcolm S. and Schoetz, Eva-Maria},
  date = {2010},
  journaltitle = {Proceedings of the National Academy of Sciences},
