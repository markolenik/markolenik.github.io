@article{grandedefranca2023,
  title = {The Role of Dietary Strategies in the Modulation of Hallmarks of Aging},
  author = {Grande de França, Natasha A. and Rolland, Yves and Guyonnet, Sophie and family=Souto Barreto, given=Philipe, prefix=de, useprefix=true},
  date = {2023-06-01},
  journaltitle = {Ageing Research Reviews},
  shortjournal = {Ageing Research Reviews},
  volume = {87},
  pages = {101908},
  issn = {1568-1637},
  doi = {10.1016/j.arr.2023.101908},
  urldate = {2023-03-31},
  abstract = {The hallmarks of aging constitute an interconnected network of basic mechanisms that modulate aging and can be modulated by lifestyle factors, including dietary strategies. This narrative review aimed to summarize the evidence on promoting dietary restriction or adherence to specific dietary patterns on hallmarks of aging. Studies with preclinical models or humans were considered. Dietary restriction (DR), usually operationalized as a reduction in caloric intake, is the main strategy applied to study the axis diet-hallmarks of aging. DR has been shown to modulate mainly genomic instability, loss of proteostasis, deregulating nutrient sensing, cellular senescence, and altered intercellular communication. Much less evidence exists on the role of dietary patterns, with most of the studies evaluating the Mediterranean Diet and other similar plant-based diets, and the ketogenic diet. Potential benefits are described in genomic instability, epigenetic alterations, loss of proteostasis, mitochondrial dysfunction, and altered intercellular communication. Given the predominant place of food in human life, it is imperative to determine the impact of nutritional strategies on the modulation of lifespan and healthspan, considering applicability, long-term adherence, and side effects.},
  langid = {english},
  keywords = {aging,Caloric restriction,diet,Dietary patterns,Fasting,Food intake,Hallmarks of aging,nutrition},
  file = {/Users/molenik/Zotero/storage/7XXSKEMH/grandedefranca2023.pdf;/Users/molenik/Zotero/storage/BGK5P8W5/S1568163723000673.html}
}

